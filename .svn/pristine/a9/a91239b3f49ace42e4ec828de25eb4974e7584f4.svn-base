.plupload_queue_container .btn_up { background: url('../img/icon_plus.png') no-repeat; cursor: pointer; display: inline-block; height: 60px; width: 60px; overflow: hidden; }
.plupload_queue_preview img { border: 1px solid #ddd; border-radius: 2px; height: 100px; width: 100px; overflow: hidden; background: url('../img/throbber.gif') center no-repeat; outline: 0; }
.plupload_queue_preview { display: inline-block; float: left; max-width: 600px; }
.plupload_queue_container { display: inline-block; vertical-align: top; float: left; margin-right: 10px;  }
.plupload_queue_preview .plupload_file_action { float: none;height: 14px;margin-left: 0;position: absolute;right: 0px;text-align: left;top: 0px;width: 13px; }
.plupload_queue_preview .plupload_file_action a.plupload_delete { background: url('../img/icon_close.png'); display: inline-block; height: 14px;width: 13px;z-index:10; }
.plupload_queue_preview .plupload_file_action a.plupload_failed { background: url('../img/error.gif'); }
.plupload_queue_preview ul { list-style: none; margin: 0; overflow: auto; padding: 0;max-height: 224px; }
.plupload_queue_preview ul li { float: left; margin-right: 10px; margin-top: 8px; position: relative; width: 100px; }
.plupload_queue_preview ul li.plupload_file_li { background-color: #e6e6e6; height: 90px; padding: 6px; width: 100px; }
.plupload_queue_preview ul li.plupload_file_li a { color: #000; text-decoration: none; }
.plupload_queue_preview ul li.plupload_file_li .plupload_file_dummy { background-color: #fff; height: 70px; width: 100%; font-size: 21px; font-weight: bold; overflow: hidden; line-height: 70px; text-align: center; }
.plupload_queue_preview ul li.plupload_file_li .plupload_file_dummy span { opacity: .35; filter: Alpha(Opacity=35); background-image: none; }
.plupload_queue_preview ul li.plupload_file_li .plupload_file_name { font-weight: bold; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; }
.plupload_queue_preview ul li.plupload_file_li .plupload_file_name span { font-size: 12px; color: #555; }